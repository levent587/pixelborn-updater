{
  "name": "@pixelborn-updater/app",
  "main": "./out/main/index.js",
  "type": "module",
  "version": "0.4.1",
  "author": "levent587",
  "repository": {
    "type": "git",
    "url": "https://github.com/levent587/pixelborn-updater.git"
  },
  "scripts": {
    "dev": "vite --port 3000",
    "start": "vite --port 3000",
    "serve": "vite preview",
    "test": "vitest run",
    "electron:start": "electron-vite preview",
    "electron": "electron-vite dev",
    "build": "electron-vite build",
    "app:dir": "electron-builder --dir",
    "app:dist": "electron-builder",
    "build:win": "npm run build && electron-builder --win --config electron-builder.yml",
    "build:mac": "npm run build && electron-builder --mac --config electron-builder.yml",
    "build:linux": "npm run build && electron-builder --linux --config electron-builder.yml",
    "deploy:win": "dotenvx run --env-file .env -- powershell -Command  \"npm run build; electron-builder --win --config electron-builder.yml --publish always\"",
    "debug": "electron . --enable-logging --trace-startup"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.45.1",
    "@radix-ui/react-checkbox": "^1.3.2",
